{
  "name": "blazzi-starter-react-vite",
  "version": "0.0.1",
  "description": "Blazzi Starter Template - React Vite",
  "keywords": [
    "blazzi",
    "react",
    "vite",
    "starter",
    "template",
    "boilerplate"
  ],
  "author": "Mehmetcan YILMAZ",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.14.2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint . --max-warnings 0 --ext .js,.cjs,.mjs,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.cjs,.mjs,.jsx,.ts,.tsx --fix",
    "format": "prettier --write . --list-different",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@blazzi/eslint-config": "0.0.4",
    "@blazzi/prettier-config": "0.0.2",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.56.0",
    "prettier": "3.2.4",
    "typescript": "5.3.3",
    "vite": "5.0.12"
  }
}
